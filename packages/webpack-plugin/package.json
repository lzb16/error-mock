{
  "name": "@error-mock/webpack-plugin",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lzb16/error-mock.git",
    "directory": "packages/webpack-plugin"
  },
  "bugs": {
    "url": "https://github.com/lzb16/error-mock/issues"
  },
  "homepage": "https://github.com/lzb16/error-mock#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./runtime": {
      "types": "./dist/runtime.d.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "tsup src/index.ts src/runtime.ts --format esm,cjs --dts",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@error-mock/plugin": "workspace:*"
  },
  "peerDependencies": {
    "webpack": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "webpack": "^5.89.0"
  }
}
