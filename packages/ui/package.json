    "fast-deep-equal": "^3.1.3",
    "nodemon": "^3.1.11",
    "postcss": "^8.4.32",
    "svelte-preprocess": "^6.0.3",
    "tailwindcss": "^3.4.0",
    "tsup": "^8.0.0",
    "vite": "^5.0.10",
    "vite-plugin-dts": "^3.7.0"
